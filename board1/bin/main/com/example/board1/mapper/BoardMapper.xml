<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.example.board1.mapper.BoardMapper">
	<insert id="write">
		INSERT INTO board
		(user, title, contents, insert_date)
		VALUES
		(#{user},#{title},#{contents},#{insert_date})
	</insert>
	<select id = "boardList" resultType="Board">
		SELECT * FROM board
	</select>
</mapper>